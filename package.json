    "@angular/core": "^4.2.1",
    "@angular/forms": "^4.2.1",
    "@angular/http": "^4.2.1",
    "@angular/platform-browser": "^4.2.1",
    "@angular/platform-browser-dynamic": "^4.2.1",
    "@angular/platform-server": "^4.2.1",
    "@angular/router": "^4.2.1",
    "@ngtools/webpack": "^1.2.10",
    "bootstrap": "^4.0.0-alpha.6",
    "core-js": "^2.4.1",
    "express": "^4.14.1",
    "font-awesome": "^4.7.0",
